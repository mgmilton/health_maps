<%= form_for location_path do %>
  <div class="form-group row">
    <label class="col-sm-2 col-form-label">
      <%=label_tag :category  %></label></br>
    <div class="col-sm-10">
      <%=select_tag :category, options_for_select([["Health Outcomes"], ["Prevention"], ["Unhealthy Behavior"]]) %>
    </div>
  </div>
  <div class="form-group row">
    <label class="col-sm-2 col-form-label">
      <%=label_tag :measurement%></label></br>
    <div class="col-sm-10">
      <%=select_tag :category, options_for_select([["Arthritis"], ["Current Asthma"], ["High Blood Pressure"], ["Cancer"], ["High Cholesterol"], ["Chronic Kidney Disease"], ["Diabetes"], ["Mental Health"], ["Physical Health"], ["Teeth Loss"], ["Stroke"], ["Health Insurance"], ["Annual Checkup"], ["Dental Visit"], ["Taking BP Medication"], ["Cholesterol Screening"], ["Mammography"], ["Pap Smear Test"], ["Colorectal Cancer Screening"], ["Core prevention services for men"], ["Core prevention services for women"], ["Binge Drinking"], ["Current Smoking"], ["Physical Activity"], ["Obesity"], ["Sleep"]])%>
    </div>
  </div>
<% end %>
